{
  "family": "hhplus-java-dev",
  "containerDefinitions": [
    {
      "name": "service-dev",
      "image": "211125707908.dkr.ecr.ap-northeast-2.amazonaws.com/hhplus-dev",
      "cpu": 0,
      "portMappings": [
        {
          "name": "service-dev-8080-tcp",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/hhplus-java-dev",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::211125707908:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "compatibilities": [
    "FARGATE"
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}